{
    "framework": "nextjs",
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "outputDirectory": ".next",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://askmynotes-backend.onrender.com/api/:path*"
        },
        {
            "source": "/socket.io/:path*",
            "destination": "https://askmynotes-backend.onrender.com/socket.io/:path*"
        }
    ]
}